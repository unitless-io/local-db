{
  "name": "@unitless-io/local-db",
  "version": "1.0.0",
  "description": "Unitless Local DB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsc -w & tsc-alias -w",
    "build": "tsc && tsc-alias",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext ts --resolve-plugins-relative-to . && npm run prettier-check",
    "prettier": "prettier --ignore-path .eslintignore --write \"./**/*.ts\" \"./**/*.json\"",
    "prettier-check": "prettier --ignore-path .eslintignore --check \"./**/*.ts\" \"./**/*.json\"",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unitless-io/local-db.git"
  },
  "contributors": [
    {
      "name": "Vlad Surahin",
      "email": "v.suragin@gmail.com",
      "url": "https://github.com/vinlaud"
    },
    {
      "name": "Siarhei Smantsar",
      "email": "siarhei.smantsar@gmail.com",
      "url": "https://github.com/SIARHEI-SMANTSAR"
    }
  ],
  "license": "Apache-2.0 license",
  "bugs": {
    "url": "https://github.com/unitless-io/local-db/issues"
  },
  "homepage": "https://github.com/unitless-io/local-db#readme",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "ramda": "^0.28.0",
    "uuid": "^9.0.0"
  }
}
